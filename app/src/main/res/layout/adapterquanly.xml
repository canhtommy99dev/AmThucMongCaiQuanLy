<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imgNhaHang"
                android:layout_width="134sp"
                android:layout_height="120sp"
                android:layout_marginTop="10sp"
                android:layout_marginLeft="10dp"
                android:src="@drawable/ic_image_black_24dp" />
            <TextView
                android:layout_marginLeft="15sp"
                android:layout_marginBottom="15sp"
                android:layout_alignParentBottom="true"
                android:id="@+id/timeUpdate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="10sp"
                android:layout_marginTop="130sp"
                android:text="Time Create" />

            <TextView
                android:layout_marginTop="20dp"
                android:layout_marginLeft="150sp"
                android:id="@+id/txtTenCH"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Tên Cửa Hàng" />

            <TextView
                android:layout_marginTop="40sp"
                android:layout_marginLeft="150sp"
                android:id="@+id/txtDiaChi"
                android:layout_width="match_parent"
                android:layout_height="50sp"
                android:text="Địa Chỉ" />


            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Thông tin cửa hàng:"
                android:layout_marginTop="90sp"
                android:layout_marginLeft="150sp"/>

            <TextView
                android:id="@+id/txttimeOpenEnd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="-- Thời Gian Mở"
                android:layout_marginTop="110sp"
                android:layout_marginLeft="150sp"/>
            <TextView
                android:id="@+id/txtPhoneNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="-- Số Điện Thoại"
                android:layout_marginTop="130sp"
                android:layout_marginLeft="150sp"
                />
            <TextView
                android:id="@+id/txtShip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="-- Ship đồ ăn"
                android:layout_marginTop="150sp"
                android:layout_marginLeft="150sp"
                />

            <TextView
                android:id="@+id/txtFacebookCH"
                android:layout_width="match_parent"
                android:layout_height="50sp"
                android:text="Facebook:"
                android:layout_marginTop="170sp"
                android:layout_marginLeft="150sp"/>
            <TextView
                android:id="@+id/txtcreateby"
                android:layout_marginTop="220sp"
                android:layout_marginLeft="150sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Người Đăng:" />

</RelativeLayout>